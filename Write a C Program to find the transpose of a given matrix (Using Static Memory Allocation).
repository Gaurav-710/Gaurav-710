#include<stdio.h>
void main()
{int R,C;
printf("enter no rows");
scanf("%d",&R);
printf("enter no column");
scanf("%d",&C);
int arr1[10][10];
int arr2[10][10];

int i;
int j;
printf("enter  matrix");
for(i=0;i<R;i++)
{
     for(j=0;j<C;j++)
       {

        scanf("%d",&arr1[i][j]);     

      }


} 
for(i=0;i<R;i++)
{ 
     for(j=0;j<C;j++)
       {
 printf("%d  ",arr1[i][j]);

      }
    printf("\n");

}
 
printf("transpose of matrix");
for(i=0;i<R;i++)
{
     for(j=0;j<C;j++)
       {
        arr2[i][j]=arr1[j][i];
        printf("%d  ",arr2[i][j]);

      }
   printf("\n");

}
}
